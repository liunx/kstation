@startuml
!$font_size = 40
skinparam Component {
    FontSize $font_size
    BorderThickness 1.2
    Style rectangle
}

skinparam Rectangle {
    BorderThickness 0
}

scale 2/1
rectangle {
    component "move single page" as c1
    component "move_freepages_block()" as c2

    c1-down[hidden]--c2
}

@enduml