@startuml
scale 2/1
skinparam Activity {
    FontSize 20
    BorderThickness 1.2
    BackgroundColor transparent
}

skinparam Note {
    FontSize 16
    BorderThickness 0
    BackgroundColor transparent
}

skinparam Title {
    FontSize 16
}

title migrate_page_move_mapping()

if (!mapping) then (Yes)
    #LightBlue:(1)
else (No)
endif
if (swapbacked) then (Yes)
    #LightGreen:(2)
else (No)
    #LightYellow:(3)
endif
detach

@enduml
